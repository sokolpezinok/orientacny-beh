    "@capacitor/local-notifications": "^5.0.7",
    "@capacitor/share": "^5.0.6",
    "@capacitor/status-bar": "^5.0.4",
    "@ionic/react": "^7.1.0",
    "@ionic/react-router": "^7.1.0",
    "capacitor-secure-storage-plugin": "^0.9.0",
    "classnames": "^2.3.2",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "firebase": "^10.10.0",
    "next": "^13.5.4",
    "pullstate": "^1.25.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
