{
  "name": "orientacny-beh",
  "version": "2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "compile-android": "cap sync && cap open android",
    "android": "next build && cap run android",
    "add-android-platform": "cap add android && npm run insert-splashscreen && npm run insert-notifications",
    "insert-splashscreen": "cp --verbose -r ./resources/* ./android/app/src/main/res/",
    "insert-notifications": "python3 ./insert-manifest.py && cp --verbose ./google-services.json ./android/app/google-services.json"
  },
  "dependencies": {
    "@capacitor-firebase/messaging": "^5.4.1",
